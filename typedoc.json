{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "./docs",
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "**/node_modules/**"],
  "excludePrivate": true,
  "excludeInternal": true,
  "includeVersion": true,
  "readme": "none",
  "name": "Cents - API Documentation",
  "blockTags": [
    "@fileoverview",
    "@component",
    "@description",
    "@example",
    "@see",
    "@since",
    "@todo",
    "@module",
    "@param",
    "@returns",
    "@default"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "validation": {
    "notExported": false,
    "invalidLink": false
  }
}
